{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"importHelpers": true,
		"paths": {
			"tslib" : ["./node_modules/tslib/tslib.d.ts"]
		},
	},
	"include": ["src"]
}